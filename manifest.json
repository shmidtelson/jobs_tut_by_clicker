{
  "manifest_version": 2,

  "name": "Tut.by clicker",
  "description": "Tut.by clicker refresher",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": [
        "*://*.jobs.tut.by/applicant/resumes*",
        "*://jobs.tut.by/applicant/resumes*"
      ],
      "js": [
        "assets/js/clicker.js"
      ]
    }
  ],

  "icons": {
    "48": "icon.png"
  },
  "permissions": ["tabs"]
}
